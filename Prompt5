import math

def main():
    num_points = 1000
    x_min = 0.0
    x_max = 2.0
    step = (x_max - x_min) / (num_points - 1)

    print("x\t\tsin(x)")
    print("---------------------")

    for i in range(num_points):
        x = x_min + i * step
        print(f"{x:.6f}\t{math.sin(x):.6f}")

main()
